{
  "name": "trick-or-treat-game",
  "version": "1.0.0",
  "description": "game to collect treats on the board in minimum moves",
  "keywords": [
    "testing",
    "game"
  ],
  "main": "src/index.js",
  "dependencies": {
    "puppeteer": "^1.20.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-scripts": "1.1.4",
    "react-testing-library": "5.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
